{ 
  "plugins": ["plugins/markdown"], 
  "recurseDepth": 10, 
  "source": { 
    "include": ["controllers", "middleware"], 
    "includePattern": ".js$", 
    "excludePattern": "(node_modules/|docs)"
  }, 
  "templates": { 
    "cleverLinks": true, 
    "monospaceLinks": true
  }, 
  "opts": { 
    "destination": "./docs/", 
    "recurse": true, 
    "readme": "./readme.md"
  } 
} 